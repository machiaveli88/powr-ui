{
   "parser": "babel-eslint",
   "extends": "eslint-config-airbnb",
   "plugins": [
      "react"
   ],
   "env": {
      "browser": true,
      "node": true,
      "mocha": true
   },
   "globals": {
      "chai": true
   },
   "rules": {
      "one-var": 0,
      "no-var": 1,
      "space-before-function-paren": 1,
      "new-cap": 1,
      "semi": 1,
      "spaced-line-comment": 1,
      "no-undef": 0,
      "block-scoped-var": 0,
      "no-new": 0,
      "no-unused-vars": 0,
      "new-parens": 1,
      "consistent-return": 1,
      "key-spacing": 1,
      "curly": 1,
      "no-shadow": 1,
      "semi-spacing": 1,
      "space-before-blocks": 1,
      "camelcase": 1,
      "space-after-keywords": 1,
      "no-extra-bind": 1,
      "no-empty": 1,
      "no-script-url": 1,
      "eol-last": 1,
      "vars-on-top": 1,
      "padded-blocks": 1,
      "guard-for-in": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1,
      "quotes": [
         1,
         "double",
         "avoid-escape"
      ],
      "comma-dangle": [
         1,
         "never"
      ],
      "brace-style": [
         1,
         "stroustrup",
         {
            "allowSingleLine": false
         }
      ],
      "no-underscore-dangle": 0,
      "space-in-brackets": 0,
      "func-names": 0,
      "no-else-return": 0,
      "no-param-reassign": 0,
      "no-reserved-keys": 0,
      "indent": [
         1,
         3
      ]
   }
}
